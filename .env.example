# Tavily API - Company and news research
# Get your key at: https://tavily.com
TAVILY_API_KEY=your_tavily_api_key_here

# Firecrawl API - Website scraping and content extraction
# Get your key at: https://firecrawl.dev
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Apollo.io API - Contact enrichment
# Get your key at: https://apollo.io
APOLLO_API_KEY=your_apollo_api_key_here

# Mock Mode - Set to true to use fake data and avoid API calls (for testing)
# When enabled, API keys can be set to any value
MOCK_MODE=false

# Optional: Configure API behavior
# TAVILY_MAX_RESULTS=5
# FIRECRAWL_MAX_PAGES=10
# APOLLO_MAX_CONTACTS=10
